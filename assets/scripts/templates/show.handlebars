
  <section data-id="{{election.id}}">
    {{log election}}
    <h6>Name:</h6>
    <h3>
      {{limit election.name 15}}
    </h3>
    <h6>Voting method:</h6>
    <p>
      {{#if election.voting_method}}
        {{election.voting_method}}
      {{else}}
        No voting method provided.
      {{/if}}
    </p>
    <h6>Option 1:</h6>
    <p>
      {{#if election.option1}}
        {{election.option1}}
      {{else}}
        No options provided.
      {{/if}}
    </p>
    {{#if election.option2}}
    <h6>Option 2:</h6>
    <p>
        {{election.option2}}
      </p>
      {{/if}}
    {{#if election.option3}}
    <h6>Option 3:</h6>
    <p>
        {{election.option3}}
      </p>
      {{/if}}
    {{#if election.option4}}
    <h6>Option 4:</h6>
    <p>
        {{election.option4}}
      </p>
      {{/if}}
    {{#if election.option5}}
    <h6>Option 5:</h6>
    <p>
        {{election.option5}}
      </p>
      {{/if}}
    {{#if election.option6}}
    <h6>Option 6:</h6>
    <p>
        {{election.option6}}
      </p>
      {{/if}}
    {{#if election.option7}}
    <h6>Option 7:</h6>
    <p>
        {{election.option7}}
    </p>
    {{/if}}
    {{#if election.option8}}
    <h6>Option 8:</h6>
    <p>
        {{election.option8}}
    </p>
    {{/if}}
    {{#if election.option9}}
    <h6>Option 9:</h6>
    <p>
        {{election.option9}}
    </p>
    {{/if}}
    <div class='buttons'>
      <button type="submit" class="btn updateElectionButton">Edit</button>
      <button type="submit" class="btn btn-danger delete-election">Delete</button>
      <button class="index-elections btn btn-info mr-2">Back</button>
    </div>

    <!-- {{> partial}} -->
  </section>
